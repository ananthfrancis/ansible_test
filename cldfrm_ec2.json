{
  "AWSTemplateFormatVersion":"2010-09-09",
  "Resources": {
    "Ec2Instance": {
      "Type": "AWS::EC2::Instance",
      "Properties": {
        "ImageId" : "ami-ced77aad",
	"KeyName" : "immutable"
      }
    }
  },
  "Outputs": {
    "JenkinsServerDNSName": {
      "Description": "DNS Name of Jenkins Server",
      "Value": {
        "Fn::GetAtt": [
          "Ec2Instance",
          "PublicDnsName"
        ]
      }
    }
  }
}
